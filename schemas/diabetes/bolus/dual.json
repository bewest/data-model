			"type":"string",
			"id": "http://jsonschema.net/deviceTime",
			"required":true
		},
		"duration": {
			"type":"number",
			"id": "http://jsonschema.net/duration",
			"required":true
		},
		"extendedDelivery": {
			"type":"number",
			"id": "http://jsonschema.net/extendedDelivery",
